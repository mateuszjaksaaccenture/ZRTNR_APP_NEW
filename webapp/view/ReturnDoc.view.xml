<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar"
	controllerName="cre.ret.app.controller.ReturnDoc">
	<semantic:FullscreenPage id="page" navButtonPress="onNavBack" showNavButton="true" title="{i18n>returnTitle}" busy="{returnView>/busy}"
		busyIndicatorDelay="{returnView>/delay}">
		<semantic:content>
			<ObjectHeader id="objectHeader" responsive="{device>/system/phone}" fullScreenOptimized="{device>/system/phone}" title="{SalesDoc}"
				number="{ path: 'GrossVal', formatter: '.formatter.numberUnit' }" numberUnit="{Currency}">
				<attributes>
					<ObjectAttribute title="{i18n>DELIV}" text="{Delivery}"/>
					<ObjectAttribute title="{i18n>CUST_NUM}" text="{CustNum}"/>
					<ObjectAttribute title="{i18n>FULL_NAME}" text="{CustDescr}"/>
				</attributes>
			</ObjectHeader>
		</semantic:content>
	</semantic:FullscreenPage>
</mvc:View>